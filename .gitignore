# This .gitignore file should be placed at the root of your Unity project directory
#
# Get latest from https://github.com/github/gitignore/blob/main/Unity.gitignore
#
/gecko-puzzle-unity/.utmp/
/gecko-puzzle-unity/[Ll]ibrary/
/gecko-puzzle-unity/[Tt]emp/
/gecko-puzzle-unity/[Oo]bj/
/gecko-puzzle-unity//[Bb]uild/
/gecko-puzzle-unity//[Bb]uilds/
/gecko-puzzle-unity//[Ll]ogs/
/gecko-puzzle-unity//[Uu]ser[Ss]ettings/
/gecko-puzzle-unity/*.log

# By default unity supports Blender asset imports, *.blend1 blender files do not need to be commited to version control.
/gecko-puzzle-unity/*.blend1
/gecko-puzzle-unity/*.blend1.meta

# MemoryCaptures can get excessive in size.
# They also could contain extremely sensitive data
/gecko-puzzle-unity/[Mm]emoryCaptures/

# Recordings can get excessive in size
/gecko-puzzle-unity/[Rr]ecordings/

# Uncomment this line if you wish to ignore the asset store tools plugin
# /[Aa]ssets/AssetStoreTools*

# Autogenerated Jetbrains Rider plugin
/gecko-puzzle-unity/[Aa]ssets/Plugins/Editor/JetBrains*
# Jetbrains Rider personal-layer settings
/gecko-puzzle-unity/*.DotSettings.user

# Visual Studio cache directory
/gecko-puzzle-unity/.vs/

# Gradle cache directory
/gecko-puzzle-unity/.gradle/

# Autogenerated VS/MD/Consulo solution and project files
/gecko-puzzle-unity/ExportedObj/
/gecko-puzzle-unity/.consulo/
/gecko-puzzle-unity/*.csproj
/gecko-puzzle-unity/*.unityproj
/gecko-puzzle-unity/*.sln
/gecko-puzzle-unity/*.suo
/gecko-puzzle-unity/*.tmp
/gecko-puzzle-unity/*.user
/gecko-puzzle-unity/*.userprefs
/gecko-puzzle-unity/*.pidb
/gecko-puzzle-unity/*.booproj
/gecko-puzzle-unity/*.svd
/gecko-puzzle-unity/*.pdb
/gecko-puzzle-unity/*.mdb
/gecko-puzzle-unity/*.opendb
/gecko-puzzle-unity/*.VC.db

# Unity3D generated meta files
/gecko-puzzle-unity/*.pidb.meta
/gecko-puzzle-unity/*.pdb.meta
/gecko-puzzle-unity/*.mdb.meta

# Unity3D generated file on crash reports
/gecko-puzzle-unity/sysinfo.txt

# Mono auto generated files
/gecko-puzzle-unity/mono_crash.*

# Builds
/gecko-puzzle-unity/*.apk
/gecko-puzzle-unity/*.aab
/gecko-puzzle-unity/*.unitypackage
/gecko-puzzle-unity/*.unitypackage.meta
/gecko-puzzle-unity/*.app

# Crashlytics generated file
/gecko-puzzle-unity/crashlytics-build.properties

# TestRunner generated files
/gecko-puzzle-unity/InitTestScene*.unity*

# Addressables default ignores, before user customizations
/gecko-puzzle-unity/ServerData
/gecko-puzzle-unity/[Aa]ssets/StreamingAssets/aa*
/gecko-puzzle-unity/[Aa]ssets/AddressableAssetsData/link.xml*
/gecko-puzzle-unity/[Aa]ssets/Addressables_Temp*
# By default, Addressables content builds will generate addressables_content_state.bin
# files in platform-specific subfolders, for example:
# /Assets/AddressableAssetsData/OSX/addressables_content_state.bin
/gecko-puzzle-unity/[Aa]ssets/AddressableAssetsData/*/*.bin*

# Visual Scripting auto-generated files
/gecko-puzzle-unity/[Aa]ssets/Unity.VisualScripting.Generated/VisualScripting.Flow/UnitOptions.db
/gecko-puzzle-unity/[Aa]ssets/Unity.VisualScripting.Generated/VisualScripting.Flow/UnitOptions.db.meta
/gecko-puzzle-unity/[Aa]ssets/Unity.VisualScripting.Generated/VisualScripting.Core/Property Providers
/gecko-puzzle-unity/[Aa]ssets/Unity.VisualScripting.Generated/VisualScripting.Core/Property Providers.meta

# Auto-generated scenes by play mode tests
/gecko-puzzle-unity/[Aa]ssets/[Ii]nit[Tt]est[Ss]cene*.unity*
